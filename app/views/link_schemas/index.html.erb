<h1>Listing link_schemas</h1>

<table>
  <tr>
    <th>Project</th>
    <th>From type</th>
    <th>To type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @link_schemas.each do |link_schema| %>
  <tr>
    <td><%= link_schema.project %></td>
    <td><%= link_schema.from_type %></td>
    <td><%= link_schema.to_type %></td>
    <td><%= link_to 'Show', link_schema %></td>
    <td><%= link_to 'Edit', edit_link_schema_path(link_schema) %></td>
    <td><%= link_to 'Destroy', link_schema, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Link schema', new_link_schema_path %>
