<%= form_for @link_schema, :html => { :class => 'form-horizontal' } do |f| %>
  <!--div class="control-group">
    <%= f.label :project_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :project_id, :class => 'number_field' %>
    </div>
  </div-->
  <div class="control-group">
    <%= f.label :from_type_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.collection_select :from_type_id, @types, :id, :title, {:prompt => false} %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :to_type_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.collection_select :to_type_id, @types, :id, :title, {:prompt => false} %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                link_schemas_path, :class => 'btn' %>
  </div>
<% end %>
